class t10
{ 
public static void set(int a[][], int n) 
{         
int d = 0, ad = 0; 
System.out.println("Difference between sum of diagonal& anti-diagonal elements in a matrix:");
for(int i=0;i<n;i++) 
{ 
for(int j=0;j<n;j++) 
{ 
if(i==j)  
{
d += a[i][j];
}  
if(i==(n-j-1)) 
ad+= a[i][j]; 
} } 
System.out.print(d-ad); 
}
public static void main(String args[])  
{ 
int n = 3; 
int a[][] = {{10,15,13},{11,8,5},{ 9,4,3}};
set(a, n); 
}} 
